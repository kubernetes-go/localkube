{
  "name": "easy-ci-cd",
  "description": "fast and easy to do ci and cd",
  "version": "0.0.1",
  "author": "Wu Jun",
  "bin": {
    "easy-ci-cd": "./bin/run",
    "eid": "./bin/run"
  },
  "bugs": "https://github.com/easy-programming-guide/easy-ci-cd/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "tslib": "^1.13.0",
    "js-yaml": "^3.14.0",
    "nunjucks": "^3.2.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.28",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "@types/js-yaml": "^3.12.5",
    "@types/mkdirp": "^1.0.1",
    "@types/nunjucks": "^3.1.3",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "eslint": "7.5.0",
    "copyfiles": "^2.3.0",
    "@typescript-eslint/parser": "3.7.0",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "eslint-config-prettier": "6.11.0 ",
    "eslint-plugin-prettier": "3.1.4",
    "prettier": "2.0.5 ",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-airbnb-typescript-prettier": "3.1.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/easy-programming-guide/easy-ci-cd",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "easy-ci-cd",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "easy-programming-guide/easy-ci-cd",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
